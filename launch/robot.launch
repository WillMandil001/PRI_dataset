<?xml version="1.0"?>
<launch>
    <arg name="robot_ip" default="172.16.0.2"/>
    <arg name="load_gripper" default="False"/>

    <!-- CONTROLLER -->
    <include file="$(find franka_control)/launch/franka_control.launch">
        <arg name="robot_ip" value="$(arg robot_ip)"/>
        <arg name="load_gripper" value="$(arg load_gripper)" />
    </include>

    <!-- MOVE IT -->
    <include file="$(find panda_moveit_config)/launch/panda_moveit.launch" />

    <!-- RVIZ -->
    <include file="$(find panda_moveit_config)/launch/moveit_rviz.launch" />

    <!-- CAMERA -->
    <node name="realsense_camera" pkg="PRI_dataset" type="realsense_camera.py" output="screen"/>

    <!-- á¹¢cripts -->
    <!-- <node name="state_publisher" pkg="PRI_dataset" type="state_publisher.py" output="screen"/> -->

    <!-- <node name="status_pub" pkg="topic_tools" type="throttle" args="messages /move_group/status 50"/> -->
    <!-- <node pkg="robo_pick" type="find_mushrooms.py" name="find_mushrooms"/> -->
</launch>